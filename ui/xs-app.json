{
	"welcomeFile": "/ui/index.html",
	"authenticationMethod": "none",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
		"source": "^/ui/(.*)$",
		"target": "$1",
		"localDir": "webapp"
	}, {
		"source": "/xsodata/(.*)",
		"target": "$1",
		"destination": "js_api"
	}, {
		"source": "/xsjs/(.*)",
		"target": "$1",
		"destination": "js_api"
	}]
}